{
  "name": "steplauncher",
  "version": "1.0.0",
  "description": "Launcher For Minecraft!",
  "main": "src/app.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NovaStepStudios/StepLauncher.git"
  },
  "keywords": [
    "Minecraft",
    "Minecraft-Launcher-Core",
    "Minecraft-Core-Master",
    "Forge",
    "Optifine",
    "NeoForge",
    "Fabric",
    "Clients",
    "Launcher",
    "Mc",
    "Launcher-Minecraft",
    "Electron",
    "App"
  ],
  "author": "NovaStepStudios",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/NovaStepStudios/StepLauncher/issues"
  },
  "homepage": "https://github.com/NovaStepStudios/StepLauncher#readme",
  "devDependencies": {
    "adm-zip": "^0.5.16",
    "electron": "^36.4.0",
    "electron-builder": "^26.0.12",
    "node-fecth": "^0.0.1-security"
  },
  "dependencies": {
    "discord-rpc": "^4.0.1",
    "tar": "^7.4.3",
    "unzipper": "^0.12.3"
  },
  "build": {
    "appId": "stepnicka.steplauncher",
    "productName": "StepLauncher",
    "directories": {
      "output": "release",
      "buildResources": "build"
    },
    "files": [
      "src/",
      "package.json"
    ],
    "extraMetadata": {
      "main": "src/app.js"
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "maintainer": "Stepnicka",
      "category": "Utility"
    }
  }
}
